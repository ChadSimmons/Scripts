          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          select UpdateClassification = N''
          union all
          select distinct UpdateClassification = CategoryInstanceName
          from fn_rbac_ListUpdateCategoryInstances(@lcid, @UserSIDs)
          where CategoryTypeName='UpdateClassification' and AllowSubscription=1
          order by 1
        </CommandText>
      </Query>
